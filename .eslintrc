{
    "extends": "eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "new-cap": 0,
        "strict": 0,
        "no-nunderscore-dangle": 0,
        "no-user-before-define": 0,
        "eol-last": 0,
        "quotes": [2, "double"],
        "jsx-quotes": 1,
        "react/jsx-no-undef": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1
    }
}
